curr_dir=$(pwd)


sh utilities                     # [1]
#sh flathub snap                 # [2]
sh github-setup                  # [3]
sh browsers                      # [4]
sh social                        # [5]
sh virtualization                # [6]
sh code_editors                  # [7]
sh prog_languages                # [8]
sh man_alternatives              # [9]
sh lazyvim                       # [10]
sh oh_my_zsh_setup               # [11]
sh anaconda                      # [12]

rm -rf $curr_dir/temp/